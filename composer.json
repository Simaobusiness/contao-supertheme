{
	"name":"comolo/contao-supertheme",
    "type": "symfony-bundle",
	"description":"SuperTheme is a Contao 3 module to easily add assets to a page layout.",
	"keywords":["contao","theme", "supertheme"],
	"homepage":"https://github.com/comolo/contao-supertheme",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Hendrik Obermayer, Comolo GmbH",
			"homepage":"http://www.comolo.de",
			"role":"Developer"
		}
	],
	"require": {
		"php":">=5.4.0",
        "symfony/framework-bundle": "~2.7",
        "contao/core-bundle": "~4.0",
		"leafo/scssphp": "~0.1",
		"leafo/scssphp-compass": "dev-master",
		"coffeescript/coffeescript": ">=1.3,<2.0"
	},
    "conflict": {
        "contao/core": "*"
    },
	"autoload":{
        "psr-4": {
            "Comolo\\SuperThemeBundle\\": "src/"
        }
	}
}